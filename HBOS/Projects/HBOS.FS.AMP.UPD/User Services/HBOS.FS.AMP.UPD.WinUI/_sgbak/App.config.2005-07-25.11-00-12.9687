<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="CacheManagerSettings" type="Microsoft.ApplicationBlocks.Cache.CacheConfigurationHandler,Microsoft.ApplicationBlocks.Cache" />
	</configSections>
	<appSettings>
        <!-- Import Details -->
		<add key="ImportMarketIndicesFilter" value="Market Indices Files (*.ind)|*.ind|All files (*.*)|*.*" />
		<add key="ImportCurrencyExchangeRateFilter" value="Exchange Rate Files (*.exr)|*.exr|All files (*.*)|*.*" />
		<add key="ImportHi3PricesFilter" value="Price Files (*.pro)|*.pro|All files (*.*)|*.*" />	
		<add key="ImportHi3AssetFundSplits" value="Asset Market Split Files (*.afs)|*.afs|All files (*.*)|*.*" />	
		<add key="ImportHi3CompositeFundSplits" value="Composite Fund Split Files (*.cmp)|*.cmp|All files (*.*)|*.*" />	
		<add key="ImportHi3CompositePricesFilter" value="Composite Price Files (*.prc)|*.prc|All files (*.*)|*.*" />	
		<add key="ImportHi3LinkedPricesFilter" value="Linked Price Files (*.pri)|*.pri|All files (*.*)|*.*" />	

		<!-- Switch to display dev style UI or end user style error UI -->
		<!-- 
			 "EndUser" for simple message picked up from resource files,
			 "Dev" for more complex dialog, 
			 "All" to display both dev & end user dialogs (all as opposed to both to allow for future types 
		-->
		<add key="ErrorUIStyle" value="EndUser"/>
		
		<!-- Development Server -->
		<!--<add key="ConnectionString" value="Password=upd;Persist Security Info=True;User ID=upd;Initial Catalog=UPD;Data Source=SQLSRV95;Application Name=UPD Administration;" />-->
		<!-- SAT Server -->
		<!--<add key="ConnectionString" value="Password=upd;Persist Security Info=True;User ID=upd;Initial Catalog=UPD_VAL;Data Source=SQLSRV98\FSYS2000;Application Name=UPD Administration;" />-->
		<!-- UAT Server -->
		<!--<add key="ConnectionString" value="Persist Security Info=False;Integrated Security=SSPI;Persist Security Info=False;Data Source=SSF003DEV;Initial Catalog=UPD;Application Name=UPD Administration;" />-->
		<!-- UAT Server (udp_develop uid) -->
		<add key="ConnectionString" value="Password=upd;Persist Security Info=True;User ID=upd;Initial Catalog=UPD_MVSII_LIVE;Data Source=SSF001PPD;Application Name=UPD Administration;" />
		<!--Live Server -->
		<!--<add key="ConnectionString" value="Persist Security Info=False;Integrated Security=SSPI;Persist Security Info=False;Data Source=SSF001;Initial Catalog=UPD;Application Name=UPD Administration;" />-->
		<!--<add key="ConnectionString" value="Password=loaddata;User ID=upd_dataload;Persist Security Info=False;Data Source=SSF001;Initial Catalog=UPD;Application Name=UPD Administration;" />-->
	</appSettings>
	<CacheManagerSettings>
		<!-- DATA PROTECTION SETTINGS
		Use DataProtectionInfo to set the assembly and class which implement
		the dataprotection interfaces for the cache.
		-->
		<DataProtectionInfo AssemblyName="Microsoft.ApplicationBlocks.Cache" 
							ClassName="Microsoft.ApplicationBlocks.Cache.DataProtection.DefaultDataProtection" 
							ValidationKey="Oci44OQ9C3xAdQ3/BMHpksPfzeTezLkXen/ahQ8T7nVk/KMgAFnssQJr00KUNhRso+MpLVwAinGep6i14X9M+A=="
							Validation="SHA1"/>											  
		
        <StorageInfo AssemblyName="Microsoft.ApplicationBlocks.Cache" 
                     ClassName="Microsoft.ApplicationBlocks.Cache.Storages.SingletonCacheStorage"
                     Mode="InProc"
                     Validated="true"
                     Encrypted="true"/>

 
		<!-- 
			MmfDictionarySize - It is the size (in bytes) of the dictionary object (in MmfCacheStorage) used to store the references of cache items. 
		-->
		
		<!-- SCAVENGING SETTINGS
			Use the ScavengingAlgorithm to set a class that will be executed when
			scavenging is performed.
		-->
		
		<ScavengingInfo AssemblyName="Microsoft.ApplicationBlocks.Cache" ClassName="Microsoft.ApplicationBlocks.Cache.Scavenging.LruScavenging" MemoryPollingPeriod="60" UtilizationForScavenging="80" MaximumSize="5"/>
		<!-- EXPIRATION SETTINGS
			Use the ExpirationCheckInterval to change the interval to check for 
			cache items expiration. The value attribute is represented in seconds.
		-->
		<ExpirationInfo Interval="1" />
	</CacheManagerSettings>
	
</configuration>
